for (j in 1:na) {
cons   <- grid_w[i] - grid_a[j]
if (cons > 0.0) {
obj[j, i] <- CRRA(cons, gamma) + beta*CRRA((1.0+rent)*grid_a[j], gamma)
} else {
obj[j, i] <- -10000.0
}
}
}
